{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}\\serve"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Build Tools",
            "program": "${workspaceFolder}\\build-tools\\index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Exec",
            "program": "${workspaceFolder}\\src\\app\\examples\\immutable-gamestate\\execution.ts"
        },
        {
            "name": "Current TS File",
            "type": "node",
            "request": "launch",
            "args": ["${relativeFile}"],
            "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
        }
    ]
}